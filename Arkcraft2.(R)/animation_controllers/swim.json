{
	"format_version": "1.10.0",
	"animation_controllers": {
		"controller.animation.swim": {
			"initial_state": "default",
			"states": {
				"default": {
					"transitions": [
						{
							"swim": "variable.swim_amount > 0.0"
						}
					]
				},
				"swim": {
					"animations": [
						"swimming"
					],
					"transitions": [
						{
							"default": "variable.swim_amount <= 0.0"
						}
					]
				}
			}
		}
	}
}