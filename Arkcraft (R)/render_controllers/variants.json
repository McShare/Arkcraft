{
	"format_version": "1.8.0",
	"render_controllers": {
		"controller.render.variants": {
			"arrays": {
				"textures": {
					"Array.skins": [
						"Texture.variant1",
						"Texture.variant2",
						"Texture.variant3",
						"Texture.variant4",
						"Texture.variant5"
					]
				}
			},
			"geometry": "Geometry.default",
			"materials": [
				{
					"*": "Material.default"
				}
			],
			"textures": [
				"Array.skins[query.variant]",
				"query.get_name == 'Nerva' ? Texture.variant5 : Array.skins[query.variant]" 
			]
		}
	}
} 